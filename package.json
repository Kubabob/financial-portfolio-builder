{
  "name": "financial-portfolio-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate:types": "cd shared && cargo test export_bindings",
    "dev:backend": "pnpm run generate:types && cd backend && cargo run",
    "dev:frontend": "next dev --turbopack --port 3001",
    "dev": "pnpm run dev:backend & pnpm run dev:frontend",
    "build:backend": "pnpm run generate:types && cd backend && cargo build --release",
    "build:frontend": "next build --turbopack",
    "build": "pnpm run build:backend && pnpm run build:frontend",
    "start:backend": "cd backend && cargo run --release",
    "start:frontend": "next start --port 3001",
    "start": "pnpm run start:backend & pnpm run start:frontend",
    "lint": "eslint"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/d3": "^7.4.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
